{
  "name": "rexie",
  "version": "1.0.0",
  "description": "Rexie Framework",
  "main": "./build/cjs/index.js",
  "module": "./build/esm/index.js",
  "types": "./build/types/index.d.ts",
  "exports": {
    "./*": {
      "types": "./build/types/*.d.ts",
      "require": "./build/cjs/*.js",
      "import": "./build/esm/*.js",
      "default": "./build/esm/*.js"
    },
    "./package.json": "./package.json"
  },
  "repository": "https://github.com/idev-coder/rexie.git",
  "author": "Natthapat Piasangka",
  "license": "MIT",
  "scripts": {
    "compile": "tsc -b ./tsconfig.cjs.json ./tsconfig.esm.json ./tsconfig.types.json",
    "build:clean": "rm -rf ./build",
    "build:tsc": "npm-run-all build:clean compile && node ./scripts/prepare-package-json",
    "build": "rollup -c"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/preset-react": "^7.24.7",
    "@babel/register": "^7.24.6",
    "@types/express": "^4.17.21",
    "@types/js-beautify": "^1.14.3",
    "@types/lodash.escaperegexp": "^4.1.9",
    "@types/node": "^22.2.0",
    "@types/object-assign": "^4.0.33",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "esbuild": "^0.23.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^4.20.0",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-esbuild": "^6.1.1",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "express": "^4.19.2",
    "js-beautify": "^1.15.1",
    "lodash.escaperegexp": "^4.1.2",
    "object-assign": "^4.1.1",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
